    "type": "library",
    "keywords": ["aadhaar", "validator", "data validation"],
    "authors": [
        {
            "name": "pearlkrishn",
            "email": "pearlkrishn@gmail.com"
        }
    ],
    "require": {
        "php" : "~5.6|~7"
